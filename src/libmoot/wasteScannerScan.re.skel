        URI2b                   = ([^ \t\n\r\(\)<>]*[^ \n\t\r`!\(\)\[\]{};:'\"\.,<>?«»“”‘’]|"("([^ \t\n\r\(\)<>]+|("("[^ \n\t\r\(\)<>]+")"))*")")*;
        
