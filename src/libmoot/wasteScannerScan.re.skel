	//"$SB$" (WS* "$SB$")*     { add_columns( yyleng() ); return wst_TOKEN_SB; }
    /*!re2c

	"%%"[^%\n]+"%%"          { add_columns( yyleng() ); goto std; }

	"$WB$"                   { add_columns( yyleng() ); return wst_TOKEN_WB; }
	"$SB$"                   { add_columns( yyleng() ); return wst_TOKEN_SB; }

	XML                      { add_columns( yyleng() ); return wst_TOKEN_XML; }

