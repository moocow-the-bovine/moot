        IPv4                    = ((([0]?[1-9]|"00")?[0-9]|"1"[0-9][0-9]|"2"[0-4][0-9]|"25"[0-5])"."){3}(([0]?[1-9]|"00")?[0-9]|"1"[0-9][0-9]|"2"[0-4][0-9]|"25"[0-5]);
        IP                      = IPv4|IPv6;


        MAILPART                = ([A-z0-9]|[\._\-])+;
        
        URI1a                   = (MAILPART "@")?([a-z] MAILPART ":" ("/"{1,3}|[a-z0-9%]));
        URI1b                   = ([^ \n\t\r\(\)<>]+|"("([^ \t\n\r\(\)<>]+|("("[^ \n\t\r\(\)<>]+")"))*")")*;
        URI1c                   = ("("([^ \t\n\r\(\)<>]+|("("[^ \n\t\r\(\)<>]+")"))*")"|[^ \n\t\r`!\(\)\[\]{};:'\"\.,<>?«»“”‘’]);

