	NUMBER                   { add_columns( yyleng() ); return wst_TOKEN_NUM; }

	HYPH                     { add_columns( yyleng() ); return wst_TOKEN_HYPH; }
	SC                       { add_columns( yyleng() ); return wst_TOKEN_SC; }

